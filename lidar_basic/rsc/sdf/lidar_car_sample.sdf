    <!-- camera link with sensor -->  
    <link name="camera_sensor">
        <pose relative_to='car_head'>0 0.225 0 0 0 1.5708</pose>
        <visual name='ray_sensor_visual'>
          <pose>0 0 0 0 0 0</pose>
          <geometry>
